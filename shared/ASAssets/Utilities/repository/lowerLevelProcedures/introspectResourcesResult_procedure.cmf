PROCEDURE introspectResourcesResult(
    IN dsPath				/lib/resource/ResourceDefs.ResourcePath,
	IN taskId				LONGVARCHAR,
    IN block				BIT,
    IN pageSize				INTEGER,
    IN pageStart			INTEGER,
    OUT result				CURSOR /shared/ASAssets/Utilities/repository/definitions/RepositoryDefinitions.IntrospectResultType
)
BEGIN
	DECLARE moduleName			VARCHAR DEFAULT &apos;introspectResourcesResult&apos;;
	DECLARE debug				CHAR(1) DEFAULT /shared/ASAssets/Utilities/repository/&quot;_debug&quot;.debug;
    DECLARE PROC_NAME 			CONSTANT VARCHAR(255) DEFAULT &apos;introspectResourcesResult&apos;;
    DECLARE requestXML 			LONGVARCHAR;
	DECLARE responseXML			XML;
	DECLARE response2XML		LONGVARCHAR;
	DECLARE faultResponseXML	XML;
	DECLARE namespaces			VARCHAR(1024) DEFAULT &apos;xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot; xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot;&apos;;
	DECLARE xpath				VARCHAR(1024) DEFAULT &apos;/resource:getMostRecentIntrospectionStatusResponse/resource:status/resource:status&apos;;
	DECLARE status				VARCHAR DEFAULT null;
	DECLARE i					INTEGER DEFAULT 0;
	DECLARE pos					INTEGER;
	DECLARE currentRec			/shared/ASAssets/Utilities/repository/definitions/RepositoryDefinitions.IntrospectResultType;
	DECLARE previousRec			/shared/ASAssets/Utilities/repository/definitions/RepositoryDefinitions.IntrospectResultType;
	DECLARE containerRec		/shared/ASAssets/Utilities/repository/definitions/RepositoryDefinitions.IntrospectResultType;
	DECLARE currentPath			LONGVARCHAR;
	DECLARE previousPath		LONGVARCHAR DEFAULT &apos;&apos;;
	DECLARE currentContainer	LONGVARCHAR;
	DECLARE previousContainer	LONGVARCHAR DEFAULT &apos;&apos;;

    IF (taskId IS NULL) THEN
        RAISE System.IllegalArgumentException VALUE PROC_NAME || &apos;: The input &quot;taskId&quot; may not be NULL.&apos;;
    END IF;

    IF (block IS NULL) THEN
        RAISE System.IllegalArgumentException VALUE PROC_NAME || &apos;: The input &quot;block&quot; may not be NULL.&apos;;
    END IF;

    IF (pageSize IS NOT NULL AND pageSize &lt; 1) THEN
        RAISE System.IllegalArgumentException VALUE PROC_NAME || &apos;: The input &quot;pageSize&quot; must be positive.&apos;;
    END IF;

    IF (pageStart IS NOT NULL AND pageStart &lt; 1) THEN
        RAISE System.IllegalArgumentException VALUE PROC_NAME || &apos;: The input &quot;pageStart&quot; must be positive.&apos;;
    END IF;

    SET requestXML = 
&apos;&lt;resource:introspectResourcesResult xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot; xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;
  &lt;common:taskId&gt;&apos; || CAST(XMLTEXT(taskId) AS VARCHAR(2147483647)) || &apos;&lt;/common:taskId&gt;
  &lt;common:block&gt;&apos; || block || &apos;&lt;/common:block&gt;
&apos;;

    IF (pageSize IS NOT NULL AND pageStart IS NOT NULL) THEN
        SET requestXML = requestXML ||
&apos;  &lt;common:page&gt;
    &lt;common:size&gt;&apos; || pageSize || &apos;&lt;/common:size&gt;
    &lt;common:start&gt;&apos; || pageStart || &apos;&lt;/common:start&gt;
  &lt;/common:page&gt;
&apos;;
    END IF;

    SET requestXML = requestXML || 
&apos;  &lt;resource:detail&gt;FULL&lt;/resource:detail&gt;
&lt;/resource:introspectResourcesResult&gt;&apos;;
	IF (debug = &apos;Y&apos;) THEN
		CALL PRINT(moduleName||&apos; : requestXML=&apos;||CHR(10)||requestXML);
	END IF;

	CALL /services/webservices/system/admin/resource/operations/introspectResourcesResult(requestXML, responseXML, faultResponseXML);
	IF (debug = &apos;Y&apos;) THEN
		CALL PRINT(moduleName||&apos; : introspectResourcesResult responseXML=&apos;||CHR(10)||SUBSTRING(NVL(CAST(responseXML AS LONGVARCHAR),&apos;NULL&apos;),1,63900));
	END IF;

	WHILE (status IS NULL OR LENGTH(TRIM(status)) = 0) DO
		SET i = i + 1;
		call /services/webservices/system/admin/resource/operations/getMostRecentIntrospectionStatus(&apos;
         &lt;resource:getMostRecentIntrospectionStatus xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot; xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;
           &lt;resource:path&gt;&apos;||dsPath||&apos;&lt;/resource:path&gt;
           &lt;resource:detail&gt;FULL&lt;/resource:detail&gt;
         &lt;/resource:getMostRecentIntrospectionStatus&gt;&apos;, responseXML, faultResponseXML);
		-- DO NOT CHANGE THIS LINE.  IT FORCES CIS TO COMPLETE THE RESULTS.
		SET response2XML = CAST(responseXML AS LONGVARCHAR);
		IF (debug = &apos;Y&apos;) THEN
			CALL PRINT(moduleName||&apos; : status=&apos;||NVL(status,&apos;NULL&apos;)||&apos;  getMostRecentIntrospectionStatus responseXML=&apos;||CHR(10)||SUBSTRING(NVL(response2XML,&apos;NULL&apos;),1,63900));
		END IF;
		CALL /shared/ASAssets/Utilities/&quot;xml&quot;/getValueFromXML(&apos;N&apos;, namespaces, xpath, response2XML, status);
		IF (debug = &apos;Y&apos;) THEN
			CALL PRINT(moduleName||&apos; : status=&apos;||NVL(status,&apos;NULL&apos;));
		END IF;
	END WHILE;
	IF (debug = &apos;Y&apos;) THEN
		CALL PRINT(moduleName||&apos; : Number of getMostRecentIntrospectionStatus() loop executions=&apos;||CAST(i AS VARCHAR));
	END IF;

	-- Return a sorted list of results with no null records present
	OPEN result FOR
        SELECT * FROM /shared/ASAssets/Utilities/repository/lowerLevelProcedures/introspectResourcesResultXSLT(response2XML)
			WHERE &quot;path&quot; IS NOT NULL OR &quot;type&quot; IS NOT NULL OR subtype IS NOT NULL OR &quot;action&quot; IS NOT NULL
			ORDER BY LOWER(&quot;path&quot;);
END</attribute>
