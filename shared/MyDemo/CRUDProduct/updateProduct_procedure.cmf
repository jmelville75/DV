  <attribute name="Script" type="STRING">-- CIS Procedure: updateProduct
--
-- DESC: Update a product view with supplied values. This example assumes all values 
--	 are supplied by the caller.
--
-- USAGE: Publish this procedure as a web service and call it using a SOAP request.

PROCEDURE updateProduct(
	IN	InProductID		INTEGER,
	IN	InProductName		VARCHAR(50),
	IN	InProductDescription	VARCHAR(255),
	IN	InCategoryID		INTEGER,
	IN	InSerialNumber		VARCHAR(50),
	IN	InUnitPrice		DECIMAL(12,2),
	IN	InReorderLevel		INTEGER,
	IN	InLeadTime		VARCHAR(30)	
)
BEGIN

	UPDATE /shared/MyDemo/CRUDProduct/Product_View 
		SET 	ProductName 		= InProductName,
			ProductDescription 	= InProductDescription,
			CategoryID 		= InCategoryID,
			SerialNumber		= InSerialNumber,
			UnitPrice		= InUnitPrice,
			ReorderLevel		= InReorderLevel,
			LeadTime		= InLeadTime
	WHERE ProductID = InProductID;

END</attribute>
